{
  "name": "react-serverless-kickstart",
  "version": "1.0.0",
  "description": "A kickstarter pack for creating react webpages without the need for a node server. Edit",
  "main": "src/index.jsx",
  "scripts": {
    "lint": "eslint source/ webpack.*.js --cache",
    "test": "jest",
    "start": "webpack-dev-server --config webpack.dev.js --hot --progress",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "clean": "node ./scripts/clean.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianshowell/react-serverless-kickstart"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testRegex": "/__tests__/.*\\.(js|jsx)$",
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "author": "Ian Howell",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^21.2.1",
    "mocha": "^3.2.0",
    "path": "^0.12.7",
    "source-map-loader": "^0.2.1",
    "uglify-js": "^2.8.29",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "immutable": "^3.8.1",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6"
  }
}
